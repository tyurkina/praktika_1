@startuml
class Пользователь {
    - id: int
    - имя: string
    - email: string
    + зарегистрироваться(): void
    + авторизоваться(): void
}

class Квартиросъемщик {
    - бюджет: int
    + подобратьЖилье(): void
    + снятьЖилье(): void
}

class ВладелецЖилья {
    - адресЖилья: string
    - цена: int
    + предложитьЖилье(): void
    + сдатьЖилье(): void
}

Пользователь "1" -- "0..1" Квартиросъемщик
Пользователь "1" -- "0..1" ВладелецЖилья
@enduml
